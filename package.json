{
  "name": "open-docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port=4000 --no-minify",
    "start:zh": "docusaurus start --port=4000 --no-minify --locale zh-CN",
    "build": "docusaurus build --out-dir dist",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --dir dist",
    "translation:zh": "docusaurus write-translations --locale zh-CN --override",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "author": {
    "name": "htmlin",
