{
  "name": "open-docs",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.8.0",
  "prettier": "@senojs/prettier-config",
  "scripts": {
    "prepare": "husky install",
    "docusaurus": "docusaurus",
    "dev": "PRODUCTION_ENV=development docusaurus start --host 0.0.0.0 --port=4000 --no-minify",
    "dev:zh": "PRODUCTION_ENV=development docusaurus start --host 0.0.0.0 --port=4000 --no-minify --locale zh-CN",
    "start": "PRODUCTION_ENV=production docusaurus start --host 0.0.0.0 --port=4000 --no-minify",
    "start:zh": "PRODUCTION_ENV=production docusaurus start --host 0.0.0.0 --port=4000 --no-minify --locale zh-CN",
    "build:dev": "PRODUCTION_ENV=development docusaurus build --out-dir dist",
    "build": "PRODUCTION_ENV=production docusaurus build --out-dir dist",
    "production": "PRODUCTION_ENV=production docusaurus build --out-dir dist",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --dir dist",
    "translation:zh": "docusaurus write-translations --locale zh-CN --override",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "gen-api": "docusaurus gen-api-docs all",
    "clean-api": "docusaurus clean-api-docs all",
    "rebuild-api": "pnpm run clean-api && pnpm run gen-api"
  },
  "author": {
    "name": "htmlin",
    "email": "linhuangdong@ones.cn",
    "url": "https://www.yuque.com/htmlin"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@commitlint/cli": "~17.1.2",
    "@commitlint/config-conventional": "~17.1.0",
    "@docusaurus/core": "~2.2.0",
    "@docusaurus/eslint-plugin": "~2.2.0",
    "@docusaurus/module-type-aliases": "~2.2.0",
    "@docusaurus/plugin-ideal-image": "~2.2.0",
    "@docusaurus/preset-classic": "~2.2.0",
    "@docusaurus/remark-plugin-npm2yarn": "~2.2.0",
    "@docusaurus/theme-mermaid": "2.2.0",
    "@easyops-cn/docusaurus-search-local": "~0.34.0",
    "@senojs/prettier-config": "~0.3.0",
    "@tsconfig/docusaurus": "~1.0.7",
    "@typescript-eslint/eslint-plugin": "~5.36.2",
    "@typescript-eslint/parser": "~5.36.2",
    "clsx": "~1.1.1",
    "docusaurus-plugin-openapi-docs": "1.7.3",
    "docusaurus-theme-openapi-docs": "1.7.3",
    "eslint": "~8.23.1",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-mdx": "~2.2.0",
    "eslint-plugin-react-hooks": "~4.6.0",
    "husky": "~8.0.3",
    "lint-staged": "~13.2.3",
    "prettier": "~3.0.3",
    "prism-react-renderer": "~1.3.5",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "typescript": "~4.9.5"
  }
}
